{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "schedule": ["after 1am and before 11pm"],
  "timezone": "Asia/Tokyo",
  "prConcurrentLimit": 5,
  "semanticCommits": "enabled",
  "commitMessagePrefix": "chore(deps):",
  "packageRules": [
    {
      "matchManagers": [
        "npm"
      ],
      "rangeStrategy": "bump"
    }
  ]
}